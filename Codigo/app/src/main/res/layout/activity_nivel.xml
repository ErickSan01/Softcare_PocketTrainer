<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/nivelpt"
    tools:context=".NivelActivity">

    <TextView
        android:id="@+id/textViewMeta3"
        android:layout_width="277dp"
        android:layout_height="35dp"
        android:layout_alignParentStart="false"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="false"
        android:layout_alignParentBottom="false"
        android:layout_centerInParent="true"
        android:layout_marginTop="370dp"
        android:textColor="#A2B9ED"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.537"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/exp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="103dp"
        android:layout_height="103dp"
        android:layout_alignLeft="@id/imageView7"
        android:layout_alignTop="@id/imageView7"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="126dp"
        android:background="@drawable/insignia_def"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.229"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="103dp"
        android:layout_height="102dp"
        android:layout_alignTop="@id/imageView8"
        android:layout_alignRight="@id/imageView8"
        android:layout_marginTop="126dp"
        android:background="@drawable/insignia_def"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.78"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="106dp"
        android:layout_height="108dp"
        android:layout_alignWithParentIfMissing="false"
        android:layout_below="@id/backBtnNivel"
        android:layout_alignTop="@id/textViewMeta3"
        android:layout_alignParentBottom="false"
        android:layout_marginVertical="60dp"
        android:layout_marginStart="40dip"
        android:layout_marginTop="50dp"
        android:background="@drawable/insignia_def"
        app:layout_constraintEnd_toStartOf="@+id/imageView8"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewMeta3" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="103dp"
        android:layout_height="109dp"
        android:layout_alignStart="@id/imageView7"
        android:layout_alignBottom="@id/imageView7"
        android:layout_marginStart="204dp"
        android:layout_marginTop="28dp"
        android:layout_marginBottom="0dp"
        android:background="@drawable/insignia_def"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/textViewMeta3" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="255dp"
        android:layout_height="181dp"
        android:layout_alignBottom="@+id/textViewMeta3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="127dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewMeta2"
        tools:ignore="MissingConstraints"
        tools:srcCompat="@drawable/level0" />

    <Button
        android:id="@+id/button8"
        android:layout_width="200dp"
        android:layout_height="37dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="20dp"
        android:layout_marginRight="6dp"
        android:layout_marginBottom="14dp"
        android:background="#00FFFFFF"
        android:backgroundTint="#00FFFFFF"
        android:onClick="verNivel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.966"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9" />

    <Button
        android:id="@+id/backBtnNivel"
        android:layout_width="54dp"
        android:layout_height="49dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/custombutton"
        android:text="@string/back"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/ex"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_alignWithParentIfMissing="false"
        android:layout_alignTop="@id/imageView6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="false"
        android:layout_alignParentBottom="false"
        android:layout_marginVertical="200dp"
        android:layout_marginLeft="254dp"
        android:layout_marginTop="200dp"
        android:layout_marginRight="24dp"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/exp" />

</RelativeLayout>