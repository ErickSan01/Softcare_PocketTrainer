<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/complexiones"
    tools:context=".CuerpoActivity">

    <Button
        android:id="@+id/buttonMusculo"
        android:layout_width="149dp"
        android:layout_height="242dp"
        android:layout_marginTop="136dp"
        android:layout_marginEnd="24dp"
        android:background="#00FFFFFF"
        android:onClick="robusto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonFornido"
        android:layout_width="163dp"
        android:layout_height="244dp"
        android:layout_marginTop="36dp"
        android:layout_marginBottom="73dp"
        android:background="#00FFFFFF"
        android:onClick="fornido"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonPeso" />

    <Button
        android:id="@+id/buttonPeso"
        android:layout_width="151dp"
        android:layout_height="242dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="136dp"
        android:background="#00FFFFFF"
        android:backgroundTint="#00FFFFFF"
        android:onClick="delgado"
        app:layout_constraintEnd_toStartOf="@+id/buttonMusculo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>