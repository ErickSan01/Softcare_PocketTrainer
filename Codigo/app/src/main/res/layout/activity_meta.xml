<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/goals"
    tools:context=".CuerpoActivity">

    <Button
        android:id="@+id/buttonMusculo"
        android:layout_width="152dp"
        android:layout_height="242dp"
        android:layout_marginTop="136dp"
        android:layout_marginEnd="24dp"
        android:background="#00FFFFFF"
        android:onClick="musculo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/buttonFornido"
        android:layout_width="156dp"
        android:layout_height="237dp"
        android:layout_marginBottom="82dp"
        android:background="#00FFFFFF"
        android:onClick="ambos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonPeso"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/buttonPeso"
        android:layout_width="149dp"
        android:layout_height="242dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="138dp"
        android:background="#00FFFFFF"
        android:backgroundTint="#00FFFFFF"
        android:onClick="peso"
        app:layout_constraintEnd_toStartOf="@+id/buttonMusculo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="OnClick" />

</androidx.constraintlayout.widget.ConstraintLayout>