<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bodys"
    tools:context=".AjustesCuerpoActivity">

    <Button
        android:id="@+id/backBtnAjustesCuerpo"
        android:layout_width="54dp"
        android:layout_height="49dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/buttonselector"
        android:text="@string/back"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/buttonMusculo"
        android:layout_width="158dp"
        android:layout_height="244dp"
        android:layout_marginTop="139dp"
        android:layout_marginEnd="16dp"
        android:background="#00FFFFFF"
        android:onClick="robusto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonFornido"
        android:layout_width="156dp"
        android:layout_height="247dp"
        android:layout_marginTop="24dp"
        android:background="#00FFFFFF"
        android:onClick="fornido"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonPeso" />

    <Button
        android:id="@+id/buttonPeso"
        android:layout_width="153dp"
        android:layout_height="243dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="139dp"
        android:background="#00FFFFFF"
        android:onClick="delgado"
        app:layout_constraintEnd_toStartOf="@+id/buttonMusculo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>